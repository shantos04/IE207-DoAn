{
    "name": "erp-electronics-parts",
    "private": true,
    "version": "0.1.0",
    "workspaces": [
        "client",
        "server"
    ],
    "scripts": {
        "dev": "concurrently \"npm:dev:server\" \"npm:dev:client\"",
        "dev:server": "npm --prefix server run dev",
        "dev:client": "npm --prefix client run dev",
        "seed": "npm --prefix server run seed"
    },
    "devDependencies": {
        "concurrently": "^9.1.2"
    }
}